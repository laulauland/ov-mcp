# Cloudflare Configuration
# Copy this file to .env and fill in your values

# Your Cloudflare Account ID
# Find at: https://dash.cloudflare.com/ → Select your account → Copy Account ID
CLOUDFLARE_ACCOUNT_ID=

# Cloudflare API Token
# Create at: https://dash.cloudflare.com/profile/api-tokens
# Permissions needed: Account.Workers Scripts (Edit), Account.Workers KV Storage (Edit)
CLOUDFLARE_API_TOKEN=

# KV Namespace IDs
# Create with: wrangler kv:namespace create "GTFS_CACHE"
GTFS_CACHE_KV_ID=
GTFS_CACHE_PREVIEW_KV_ID=

# Optional: Environment
ENVIRONMENT=development
