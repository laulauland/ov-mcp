{
  "name": "@ov-mcp/gtfs-parser",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun && tsc --emitDeclarationOnly",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "csv-parse": "^5.5.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "protobufjs": "^7.2.5"
  },
  "description": "Comprehensive GTFS parser with streaming support, journey planning, fuzzy search, and realtime capabilities",
  "keywords": [
    "gtfs",
    "transit",
    "public-transport",
    "amsterdam",
    "netherlands",
    "journey-planning",
    "routing",
    "realtime"
  ]
}
