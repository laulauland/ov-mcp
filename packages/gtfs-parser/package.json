{
  "name": "@ov-mcp/gtfs-parser",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun && tsc --emitDeclarationOnly",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "example:streaming": "bun run examples/streaming-example.ts",
    "example:streaming:memory": "bun run --expose-gc examples/streaming-example.ts"
  },
  "dependencies": {
    "papaparse": "^5.4.1",
    "fflate": "^0.8.2",
    "@zip.js/zip.js": "^2.7.52",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/papaparse": "^5.3.14",
    "typescript": "^5.3.3"
  }
}
